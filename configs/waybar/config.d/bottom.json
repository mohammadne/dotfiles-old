{
  "position":"bottom",
  "height":30,
  "modules-right":[
    "custom/date",
    "custom/jdate",
    "network#alvani"
  ],
  "modules-center":[
    "custom/love#relationship"
  ],
  "modules-left":[
    "network#snapp",
    "network#baly",
    "network#aut",
    "network#offerland",
    "bluetooth",
    "custom/touchpad"
  ],
  "bluetooth":{
    "format":"{icon}",
    "format-alt":"bluetooth: {status}",
    "format-icons":{
      "enabled":"",
      "disabled":""
    },
    "tooltip-format":"{}"
  },
  "network#alvani":{
    "interface":"alvani",
    "format":"{ipaddr} ",
    "format-disconnected":""
  },
  "network#snapp":{
    "interface":"snapp-*",
    "format":"{ifname}: {ipaddr}",
    "format-disconnected":""
  },
  "network#baly":{
    "interface":"baly",
    "format":"{ifname}: {ipaddr}",
    "format-disconnected":""
  },
  "network#aut":{
    "interface":"aut",
    "format":"{ifname}: {ipaddr}",
    "format-disconnected":""
  },
  "network#offerland":{
    "interface":"offerland",
    "format":"{ifname}: {ipaddr}",
    "format-disconnected":""
  },
  "custom/touchpad":{
    "interval":3,
    "exec":"$HOME/.config/waybar/scripts/touchpad.sh"
  },
  "custom/love#relationship":{
    "interval":3600,
    "exec":"$HOME/.config/waybar/scripts/couple.sh relationship",
    "format":" {}"
  },
  "custom/date":{
    "interval":3600,
    "exec":"date +'%d %B %Y'",
    "exec-if":"which date",
    "format":"{} "
  },
  "custom/jdate":{
    "interval":3600,
    "exec":"jdate +'%d %B %Y'",
    "exec-if":"which jdate",
    "format":"{} "
  }
}
